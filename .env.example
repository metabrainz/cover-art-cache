# Example alternative configuration
# Copy this to .env and modify as needed

# Service configuration
SERVICE_PORT=8000

# Use a custom cache location
COVER_ART_CACHE_DIR=/home/user/music-cache

# Set cache size limit (500MB total cache size on disk)
COVER_ART_CACHE_MAX_SIZE=500

# Cache cleanup interval in seconds (how often to check if cleanup is needed)
COVER_ART_CACHE_CLEANUP_INTERVAL=3600

# SSL and Domain Configuration
# Replace with your actual domain and email address
VIRTUAL_HOST=your-domain.com
LETSENCRYPT_HOST=your-domain.com
LETSENCRYPT_EMAIL=your-email@example.com
